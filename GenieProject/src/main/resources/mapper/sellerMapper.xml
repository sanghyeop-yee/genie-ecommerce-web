<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.genie.myapp.dao.SellerDAO">
	<select id="adminCategoryTag" resultType="AdminVO">
		select category_id, product_category from category
	</select>
	<select id="adminTag" resultType="AdminVO">
		select product_tag_id, product_tag from tag
	</select>
	<select id="adminProduct" resultType="AdminProductVO">
		select product_id, seller_id, product_category, product_tag, product_name, product_price, product_info, product_stock, product_quantity, product_hit, product_writedate, product_image1, product_image2, product_image3 from product
	</select>
	
	<select id="newOrderList"> // 주문 상품 목록 표시

	</select>
	<update id="updateDeliveryState"> // 주문 상품의 배송상태 수정
	
	</update>
	
</mapper>